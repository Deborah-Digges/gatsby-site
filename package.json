{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "npm run build:css && gatsby build",
    "develop": "npm run build:css && gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run build:css &&  npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "build:css": "tailwind build src/styles/index.css -o public"
  },
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "gatsby": "^2.24.50",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-postcss": "^2.3.11",
    "gatsby-plugin-sharp": "^2.6.31",
    "gatsby-remark-copy-linked-files": "^2.3.13",
    "gatsby-remark-images": "^3.3.28",
    "gatsby-source-filesystem": "^2.3.27",
    "gatsby-transformer-remark": "^2.8.32",
    "gatsby-transformer-sharp": "^2.5.14",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "request": "^2.88.2",
    "tailwindcss": "^1.7.6"
  },
  "devDependencies": {
    "prettier": "2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
